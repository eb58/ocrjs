{
   "name": "ocrjs",
   "version": "1.0.0",
   "keywords": [
      "ocr",
      "handwritten digits"
   ],
   "description": "ocr for handwritten digits",
   "scripts": {
      "lint": "eslint --fix js && echo 'Lint complete.'",
      "start": "node tests/main.js",
      "debug": "node --inspect-brk tests/main.js ",
      "gen-ocrebdb": "node src/db-generators/gen-ocrebdb",
      "gen-ocrmnistdb": "node src/db-generators/gen-ocrmnistdb",
      "ocrMnistImageGenerator": "node src/utils/gen-images-from-mnist",
      "test": "jest",
      "testWatch": "jest --watch",
      "changedpi": "node tests/test-changedpi.js",
      "changedpi debug": "node --inspect-brk tests/img/test-changedpi.js",
      "watch": "watch 'npm run -s lint' src"
   },
   "author": "eb",
   "contributors": [
      "eb"
   ],
   "dependencies": {
      "mkdirp": "^1.0.4",
      "pngjs": "^5.0.0",
      "pngjs-image": "^0.11.7",
      "algorithms-js-eb": "1.0.0"
   },
   "devDependencies": {
      "eslint": "7.7.0",
      "eslint-config-prettier": "6.11.0",
      "eslint-plugin-prettier": "3.1.4",
      "jest": "^26.4.2",
      "prettier": "^2.1.1",
      "watch": "^1.0.2"
   }
}
